server:
  port: 8000
spring:
  application:
      name: spring-cloud-examples-user-provider
  jpa:
    generate-ddl: false
    show-sql: true
    hibernate:
      ddl-auto: none
  datasource:
    platform: h2
    schema: classpath:schema.sql
    data: classpath:data.sql
logging:
  level:
    root: info
    org.hibernate: INFO
eureka:
  client:
    service-url:
      defaultZone: http://127.0.0.1:7878/eureka
  instance:
    prefer-ip-address: true
    lease-renewal-interval-in-seconds: 5  # 每间隔5s，向服务端发送一次心跳，证明自己依然”存活“
    lease-expiration-duration-in-seconds: 30  # 告诉服务端，如果我30s之内没有给你发心跳，就代表我“死”了，将我踢出掉